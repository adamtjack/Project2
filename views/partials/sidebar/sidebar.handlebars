<div class="d-flex align-self-stretch h-100">
    Sidebar goes here<br/>
    {{#if user}}
      User: <a href="/viewuser/{{user.userId}}">{{user.name}}</a><br/>
    {{/if}}
    {{#if categories}}
    <ul class="cat_list">
        {{#each categories}}
        <li><a href="/viewcategory/{{this.categoryId}}">{{this.categoryName}}</a></li>
        {{/each}}
    </ul>
    {{/if}}
</div>